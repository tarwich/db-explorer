    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "electron": "^35.1.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
